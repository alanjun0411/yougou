.history_title{
  height: 50px;
  padding: 10px;
  box-sizing: border-box;
  line-height: 30px;
}
.detele{
  float: right;
  color: #ccc;
  font-size: 18px;
}
.detele:active{
  color: #38f;
}
.history_content{
  display: flex;
  flex-wrap: wrap;
  font-size: 14px;
  color: #fff;
  padding: 0 10px;
}
.history_content_one{
  padding: 3px 10px;
  margin: 0 6px 6px 0;
  border-radius: 13px;
  background-color: #ccc
}